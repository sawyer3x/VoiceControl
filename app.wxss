/*设置根节点，高度占满整个屏幕 */
.page{
  height: 100%;
  width: 100%;
  position: absolute;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
  background-color: #F5F5F5;

} 
